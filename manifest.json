{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "WP Usermask",
  "description": "Set all the right default options for Honk users",
  "version": "0.2",
  "content_scripts": [
    {
    "matches": ["*://honk.nrc.nl/*"],
    "js": ["content.js"]
    }
  ],
  "background": {
  "scripts": ["background.js"]
},
  "browser_action": {
  "default_icon": "icon.png"
  }
}
